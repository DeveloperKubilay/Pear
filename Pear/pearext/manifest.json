{
  "manifest_version": 2,
  "name": "pearbrowser",
  "version": "1.0",
  "description": "pearbrowser",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "windows",
    "<all_urls>"
  ]
}