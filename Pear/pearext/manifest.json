{
  "manifest_version": 2,
  "name": "Armut",
  "version": "1.0",
  "description": "Basit bir Chrome uzantısı",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "nativeMessaging",
    "declarativeNetRequest",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "windows",
    "<all_urls>"
  ]
}